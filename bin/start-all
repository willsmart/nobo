#!/bin/bash
bindir=`realpath "$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"`
pidfile="$bindir/../../joiner.pid"
if [[ ! -f "$pidfile" ]] ; then 
  joined/bin/start-joiner &
fi
pidfile="$bindir/../../model-server.pid"
if [[ ! -f "$pidfile" ]] ; then 
  joined/bin/start-model-server --prompter &
fi
pidfile="$bindir/../../page-server.pid"
if [[ ! -f "$pidfile" ]] ; then 
  joined/bin/start-page-server &
fi
